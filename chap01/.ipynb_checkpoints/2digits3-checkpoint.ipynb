   "source": [
    "# 두 자리 양수(10~99)입력받기\n",
    "\n",
    "print('두 자리 양수를 입력하세요.')\n",
    "\n",
    "while True:\n",
    "    no = int(input('값을 입력하세요.: '))\n",
    "    if not(no < 10 or no > 99):\n",
    "        break\n",
    "        \n",
    "print(f'입력받은 양수는 {no}입니다.')"
   ]
