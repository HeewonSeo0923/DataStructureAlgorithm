   "source": [
    "# +와 -를 번갈아 출력하기 1\n",
    "\n",
    "print('+와 -를 번갈아 출력합니다.')\n",
    "n = int(input('몇개를 출력할까요 ?: '))\n",
    "\n",
    "for i in range(n):\n",
    "    if i % 2 :\n",
    "        print('-', end='')\n",
    "    else:\n",
    "        print('+', end='')\n",
    "        \n",
    "print()"
